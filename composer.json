{
    "type": "library",
    "name": "clancyhood/opensky-api",
    "description": "A PHP client to retrieve data from the OpenSky live API.",
    "version": "0.2.4",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémie BROUTIER",
            "email": "jeremie.broutier@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">= 7.1.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/validator": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "OpenSkyApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSkyApi\\Tests\\": "tests/"
        }
    }
